<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <LinearLayout
        android:id="@+id/linearColumncreateFromOne"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        style="@style/groupStyleteal_400">

        <LinearLayout
            android:id="@+id/linearHome"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/imageImagefiveOne"
                android:layout_width="313dp"
                android:layout_height="229dp"
                android:layout_gravity="center_horizontal"
                android:scaleType="fitXY"
                android:src="@drawable/img_image_2" />

            <FrameLayout
                android:id="@+id/frameStackcreatefrom"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_475pxv"
                android:gravity="center_horizontal">

                <View
                    android:id="@+id/viewCreatefromOne"
                    android:layout_width="@dimen/_375pxh"
                    android:layout_height="@dimen/_555pxh"
                    android:layout_gravity="center"
                    android:background="@drawable/rectangle_bg_white_a700_radius_bl_40_br_40" />

                <TextView
                    android:id="@+id/txtLatestnews"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="top|start"
                    android:layout_marginStart="@dimen/_19pxh"
                    android:layout_marginTop="@dimen/_135pxv"
                    android:ellipsize="end"
                    android:lineSpacingExtra="@dimen/_4pxh"
                    android:singleLine="true"
                    android:text="@string/lbl_latest_news"
                    android:textSize="15sp"
                    style="@style/txtPlusJakartaSansromanregular15" />

            </FrameLayout>

        </LinearLayout>

        <FrameLayout
            android:id="@+id/frameBottombar"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            style="@style/groupStyleteal_400cornerRadius">

            <LinearLayout
                android:id="@+id/linearRowlogoutfill0w"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginBottom="@dimen/_15pxv"
                android:layout_marginTop="@dimen/_15pxv"
                android:layout_marginLeft="@dimen/_15pxv"
                android:layout_marginRight="@dimen/_15pxv"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/imageLogout"
                    android:layout_width="@dimen/_40pxh"
                    android:layout_height="@dimen/_40pxh"
                    android:layout_margin="@dimen/_22pxh"
                    android:layout_gravity="center"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_logout_fill0_wg" />

                <ImageView
                    android:id="@+id/imageTroubleshoot"
                    android:layout_width="@dimen/_40pxh"
                    android:layout_height="@dimen/_40pxh"
                    android:layout_margin="@dimen/_22pxh"
                    android:layout_gravity="center"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_troubleshoot_fi" />

                <ImageView
                    android:id="@+id/imageDarkmode"
                    android:layout_width="@dimen/_40pxh"
                    android:layout_height="@dimen/_40pxh"
                    android:layout_margin="@dimen/_22pxh"
                    android:layout_gravity="center"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_dark_mode_fill0" />

                <ImageView
                    android:id="@+id/imageProfile"
                    android:layout_width="@dimen/_40pxh"
                    android:layout_height="@dimen/_40pxh"
                    android:layout_margin="@dimen/_22pxh"
                    android:layout_gravity="center"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_group_fill0_wgh" />

            </LinearLayout>
        </FrameLayout>

    </LinearLayout>
</layout>
