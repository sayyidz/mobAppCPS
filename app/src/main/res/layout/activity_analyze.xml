<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <LinearLayout
        android:id="@+id/linearAnalisis"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        style="@style/groupStyleteal_400">

        <FrameLayout
            android:id="@+id/frameStack"
            android:layout_width="@dimen/_313pxh"
            android:layout_height="@dimen/_218pxv"
            android:layout_gravity="center_horizontal">

            <ImageView
                android:id="@+id/imageImagefourOne"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_218pxh"
                android:layout_gravity="center"
                android:scaleType="fitXY"
                android:src="@drawable/img_image_2"
                tools:ignore="ContentDescription" />

            <androidx.appcompat.widget.Toolbar
                android:id="@+id/toolbarToolbar"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="top|center"
                app:contentInsetStart="0dp">

                <ImageButton
                    android:id="@+id/btnIconbutton"
                    android:layout_width="@dimen/_50pxh"
                    android:layout_height="@dimen/_50pxh"
                    android:layout_marginStart="@dimen/_32pxh"
                    android:layout_marginEnd="@dimen/_310pxh"
                    android:background="@drawable/rectangle_bg_white_a700_radius_16"
                    android:paddingHorizontal="@dimen/_11pxh"
                    android:paddingVertical="@dimen/_11pxv"
                    android:scaleType="fitCenter"
                    android:src="@drawable/img_group_14"
                    tools:ignore="ContentDescription" />
            </androidx.appcompat.widget.Toolbar>

        </FrameLayout>

        <LinearLayout
            android:id="@+id/linearColumnanalyze"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:paddingBottom="@dimen/_43pxv"
            android:paddingEnd="@dimen/_24pxh"
            android:paddingStart="@dimen/_24pxh"
            android:paddingTop="@dimen/_43pxv"
            style="@style/groupStylewhite_A700cornerRadius">

            <LinearLayout
                android:id="@+id/linearColumn"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_67pxh"
                android:layout_marginStart="@dimen/_77pxh"
                android:layout_marginTop="@dimen/_86pxv"
                android:orientation="vertical"
                android:paddingBottom="@dimen/_73pxv"
                android:paddingEnd="@dimen/_24pxh"
                android:paddingStart="@dimen/_24pxh"
                android:paddingTop="@dimen/_73pxv"
                style="@style/groupStyleblue_gray_100_01">

                <TextView
                    android:id="@+id/txtYourimagehere"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginTop="@dimen/_6pxv"
                    android:gravity="center_horizontal"
                    android:lineSpacingExtra="@dimen/_8pxh"
                    android:singleLine="true"
                    android:text="@string/lbl_your_image_here"
                    android:textAppearance="@style/txtPoppinsmedium16" />
            </LinearLayout>

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/btnUploadImage"
                android:layout_width="@dimen/_322pxh"
                android:layout_height="@dimen/_42pxv"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="@dimen/_82pxv"
                android:ellipsize="end"
                android:gravity="center"
                android:lineSpacingExtra="@dimen/_8pxh"
                android:paddingBottom="@dimen/_9pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_9pxv"
                android:singleLine="true"
                android:text="@string/lbl_upload_image"
                android:textAllCaps="false"
                style="@style/btnSolidRounded_2" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/btnAnalyze"
                android:layout_width="@dimen/_322pxh"
                android:layout_height="@dimen/_42pxv"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="@dimen/_40pxv"
                android:ellipsize="end"
                android:gravity="center"
                android:lineSpacingExtra="@dimen/_8pxh"
                android:paddingBottom="@dimen/_9pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_9pxv"
                android:singleLine="true"
                android:text="@string/lbl_analyze"
                android:textAllCaps="false"
                style="@style/btnSolidRounded_2" />

        </LinearLayout>

    </LinearLayout>
</layout>
