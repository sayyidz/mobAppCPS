<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <LinearLayout
        android:id="@+id/linearAnalisis"
        style="@style/groupStyleteal_400"
        android:layout_width="match_parent"
        android:layout_height="774dp"
        android:orientation="vertical">

        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbarToolbar"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="top|center"
            app:contentInsetStart="0dp" />

        <ImageButton
            android:id="@+id/back"
            android:layout_width="@dimen/_32pxh"
            android:layout_height="@dimen/_32pxh"
            android:layout_gravity="center_vertical"
            android:background="@drawable/rectangle_bg_white_a700_radius_16"
            android:paddingHorizontal="@dimen/_11pxh"
            android:paddingVertical="@dimen/_11pxv"
            android:scaleType="fitCenter"
            android:src="@drawable/img_group_14"
            tools:ignore="ContentDescription" />

        <LinearLayout
            android:id="@+id/linearColumnanalyze"
            style="@style/groupStylewhite_A700cornerRadius"
            android:layout_width="match_parent"
            android:layout_height="656dp"
            android:orientation="vertical"
            android:paddingStart="@dimen/_24pxh"
            android:paddingTop="@dimen/_43pxv"
            android:paddingEnd="@dimen/_24pxh"
            android:paddingBottom="@dimen/_43pxv">

            <ImageView
                android:id="@+id/imageView2"
                android:layout_width="match_parent"
                android:layout_height="271dp"
                tools:srcCompat="@tools:sample/avatars" />

            <TextView
                android:id="@+id/textView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text=" Predict =   % cancer" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/btnUploadImage"
                style="@style/btnSolidRounded_2"
                android:layout_width="@dimen/_322pxh"
                android:layout_height="48dp"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="@dimen/_82pxv"
                android:ellipsize="end"
                android:gravity="center"
                android:lineSpacingExtra="@dimen/_8pxh"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_9pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingBottom="@dimen/_9pxv"
                android:singleLine="true"
                android:text="@string/lbl_upload_image"
                android:textAllCaps="false" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/btnAnalyze"
                style="@style/btnSolidRounded_2"
                android:layout_width="@dimen/_322pxh"
                android:layout_height="48dp"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="@dimen/_40pxv"
                android:ellipsize="end"
                android:gravity="center"
                android:lineSpacingExtra="@dimen/_8pxh"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_9pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingBottom="@dimen/_9pxv"
                android:singleLine="true"
                android:text="@string/lbl_analyze"
                android:textAllCaps="false" />

        </LinearLayout>

    </LinearLayout>
</layout>
